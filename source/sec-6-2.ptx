            <cell><m>\hphantom{00000}</m></cell></row>
        <row bottom="minor">
            <cell><m>\vphantom{\dfrac{1}{2}}</m>20</cell>
            <cell>932</cell>
            <cell><m>\hphantom{00000}</m></cell></row>
        <row bottom="minor">
            <cell><m>\vphantom{\dfrac{1}{2}}</m>30</cell>
            <cell>2013</cell>
            <cell><m>\hphantom{00000}</m></cell></row>
        <row bottom="minor">
            <cell><m>\vphantom{\dfrac{1}{2}}</m>40</cell>
            <cell>4345</cell>
            <cell><m>\hphantom{00000}</m></cell></row>
        <row bottom="minor">
            <cell><m>\vphantom{\dfrac{1}{2}}</m>50</cell>
