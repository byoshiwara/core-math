    \draw[black] (O)++(60:\r) --(O)--++(240:\r);  
    \draw[black] (O)++(120:\r) --(O)--++(300:\r); 
    \draw[black] (O)++(150:\r) --(O)--++(330:\r); 

    \coordinate (O) at (13.5,0);
    \filldraw[green] (O)++(-180:\r) arc (-180:-150:\r)--(O)--cycle;   
    \draw[black] (O) circle (\r); 
    \node[text width=.25cm ] at (12.2,-1.6) {\color{blue}$\dfrac{13}{12}$};
    \draw[black] (O)++(-\r,0) --++(2*\r,0); 
    \draw[black] (O)++(0,-\r) --++(0,2*\r);   
    \draw[black] (O)++(30:\r) --(O)--++(210:\r);  
